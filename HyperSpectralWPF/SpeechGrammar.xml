<grammar mode="voice" version="1.0" xml:lang="en-US" root="rootRule" tag-format="semantics/1.0-literals" xmlns="http://www.w3.org/2001/06/grammar">
  <rule id="rootRule">
    <one-of>
      <item>
        <tag>FORWARD</tag>
        <one-of>
          <item> forwards </item>
          <item> forward </item>
          <item> next </item>
        </one-of>
      </item>
      <item>
        <tag>BACKWARD</tag>
        <one-of>
          <item> backward </item>
          <item> backwards </item>
          <item> back </item>
        </one-of>
      </item>
      <item>
        <tag>GOTO</tag>
        <one-of>
          <item> go to image </item>
        </one-of>
      </item>
      <item>
        <tag>WAVELENGTH</tag>
        <one-of>
          <item> go to wavelength </item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>